" -------------------------------------------------------------------------------
" ,-------------,
" | Youtube.com |
" '-------------'
group youtube -locations=youtube.com

js <<EOF
function ytplayer() {
    var player = content.document.getElementById("movie_player");
    return player && player.wrappedJSObject;
}
window.ytplayer = ytplayer;

var ytMinSpeed = 0.25;
function ytGetSpeed() { return ytplayer() && ytplayer().getPlaybackRate(); }
function ytSetSpeed(r) {
    var player = ytplayer();
    var rate = Math.max(r, ytMinSpeed);
    if (player) return ytplayer().setPlaybackRate(rate) && rate;
    else return null;
}
function ytAdjustSpeed(delta) {
    if (!ytplayer()) return null;
    var newspeed = ytGetSpeed()+delta;
    ytSetSpeed(newspeed);
    utils.yankWithMsg(`speed: ${newspeed}`);
}
function ytPlay() { return ytplayer() && ytplayer().playVideo(); }
function ytPause() { return ytplayer() && ytplayer().pauseVideo(); }
function ytIsPaused() { return (ytplayer().getPlayerState() == 2); }
function ytTogglePause() {
    if (!ytplayer()) return null;
    if (ytIsPaused()) ytPlay();
    else ytPause();
}
function ytToggleMute() {
    var p = ytplayer();
    if (!p) return null;
    if (p.isMuted()) p.unMute();
    else p.mute();
}
function ytGetTime() { return ytplayer() && ytplayer().getCurrentTime(); }
function ytForward(n) {
    var p = ytplayer();
    if (!p) return null;
    p.seekBy(n);
    utils.message(formatSeconds(ytGetTime()));
}
function ytAdjustVolume(dvol) {
    var p = ytplayer();
    if (!p) return null;
    volCurr = p.getVolume();
    volNew = cutoff(volCurr + dvol, 0, 100);
    ytShowVolumeSlider();
    p.setVolume(volNew);
    return volNew;
}
function ytShowVolumeSlider() {
    if (!ytplayer()) return null;
    var controlsDiv = content.document.querySelector('.ytp-chrome-bottom');
    controlsDiv.classList.add('ytp-volume-slider-active');
}
function ytDL(dir=downloadsdir, video=true) {
    if (!dir) dir = downloadsdir;
    var xtraArgs = video ? "--all-subs" : "-f bestaudio";
    var cmd = `!cd ${dir}; youtube-dl -ci ${xtraArgs} --xattrs -o "%(title)s-%(id)s.%(ext)s" "${buffer.URL}"`;
    dactyl.execute(cmd);
}
function ytCloseAd() {
    var imgs = Array.from(content.document.getElementsByTagName("img"));
    imgs.forEach(img=>{
        if (img.src.includes('googlesyndication'))
        imgs.forEach(e=>e.remove && e.remove());
    });
}
function ytGetPlaylists() {
    return getSelectors(".guide-item").filter(e=>e.href.match(/\/playlist\?/));
}

function ytGetPlaylistsCompletion() {
    return ytGetPlaylists().map(e=>[e.href.match(/playlist\?list=(.*)/)[1], e.textContent.trim()]);
}
function ytGetPlaylistFromTitle(title) {
    var playlists = ytGetPlaylists();
    return playlists.find(e=>e.title.trim()==title).match(/playlist\?list=(.*)/)[1];
};

function ytGetChannels() {
    return getSelectors(".guide-item").filter(e=>e.href.match(/\/channel\//));
}
function ytGetChannelsCompletion() {
    return ytGetChannels().map(e=>[e.href.match(/\/channel\/(.*)/)[1], e.textContent.trim()]);
}

function ytScreenshot() {
    "From https://github.com/ReeganExE/youtube-screenshot"
    var win = content.window;
    var doc = content.document;
    var canvas = doc.createElement("canvas");
    var video = doc.querySelector("video");
    twod = canvas.getContext("2d");
    canvas.width=parseInt(video.style.width);
    canvas.height=parseInt(video.style.height);
    twod.drawImage(video,0,0,canvas.width,canvas.height);
    win.open(canvas.toDataURL("image/jpeg"))
}

"code adapted from https://github.com/amio/youtube-screenshot-button"
function ytScreenshot2() {
    var video = content.document.querySelector('.html5-main-video');

    function createScreenshotFromVideo (video, config) {
        var canvas = content.document.createElement('canvas');
        canvas.width = video.clientWidth;
        canvas.height = video.clientHeight;

        var ctx = canvas.getContext('2d');
        ctx.drawImage(video, 0, 0, canvas.width, canvas.height);

        var mime = {
            png: 'image/png',
            jpg: 'image/jpeg'
        }[config && config.type || 'png'];

        return canvas.toDataURL(mime);
    }

    function saveDataURI(data, fname) {
        var link = content.document.createElement("a");
        content.document.body.appendChild(link);
        link.setAttribute("href", data);
        link.setAttribute("download", fname);
        link.click();
        link.remove();
    }

    var title = getFirstSelector('#container>h1.title').textContent;
    var time = video.currentTime;
    var name = `${title}-t=${time}.png`;
    var data = createScreenshotFromVideo(video);
    saveDataURI(data, name);
}

function ytUp(count) {
    clickSelectorByText("a", /Show \d+ more/) || ytAdjustSpeed(0.25 * (count || 1));
}
function ytDown(count) {
    clickSelectorByText("a", /Show fewer/) || ytAdjustSpeed(-0.25 * (count || 1));
}
EOF
