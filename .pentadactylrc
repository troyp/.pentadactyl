command! papersize
\    -description "Show paper size dimensions"
\    -nargs 1
\    -complete custom," (context) => { context.completions = paperSizes; } "
\    -js utils.message(`${args[0]}: ${paperSizes.find(e=>e[0].toUpperCase()==[args[0].toUpperCase()])[1]}`);

command! sum -nargs * -js utils.message(args.reduce((a,b)=>parseFloat(a)+parseFloat(b)));
