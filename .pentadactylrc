" -------------------------------------------------------------------------------
" ,----------,
" | Kanshudo |
" '----------'
group kanshudo -locations=kanshudo.com

js <<EOF
function kanshudoSearch(terms, where="here") {
    if (terms.length==1)
        openOrSelectTab("https://www.kanshudo.com/searchq?q="+terms[0], where);
    else
        openOrSelectTab(`https://www.kanshudo.com/searchmw?q=${terms.join("|")}&ex=y`, where);
}
function kanshudoWord(term, where="here") {
    openOrSelectTab("https://www.kanshudo.com/word/"+term, where);
}
function kanshudoKanjiSearch(terms, where="here") {
    openOrSelectTab("https://www.kanshudo.com/search?q="+terms.join(""), where);
}
function kanshudoNameSearch(term, where="here") {
    openOrSelectTab("https://www.kanshudo.com/searchn?q="+term, where);
}
function kanshudoSearchGrammar(terms, where="here") {
    openOrSelectTab("https://www.kanshudo.com/searchg?q="+terms.join(" "), where);
}
function kanshudoExampleSearch(terms, where="here") {
    openOrSelectTab("https://www.kanshudo.com/searcht?q="+terms.join(" "), where);
}
function kanshudoKanji(term, where="here") {
    openOrSelectTab("https://www.kanshudo.com/kanji/"+term, where);
}
function kanshudoKanjiSearchByComponent(terms, where="here") {
    openOrSelectTab("https://www.kanshudo.com/searchcg?q="+terms.join(""), where);
}
EOF

command! kanshudosearch -bang -nargs + -js kanshudoSearch(args, bang?"adjacent":"here");
