" | Dan Murphy's |
" '--------------'
group danmurphys -locations=danmurphys.com.au

js <<EOF
function danmurphysGo() {
    var url = buffer.URL;
    if        (url == "https://www.danmurphys.com.au/dm/mydans/my_lists") {
        clickSelectorByText("span", "Wish List");
    } else if (url.startsWith("https://www.danmurphys.com.au/product/")) {
        clickSelectorAll(".review-card__read-more");
    }
}
function danmurphysFilterProductListByStars(n) {
    function nStars(e) {
        var whole = e.querySelectorAll("span.rating-icon.checked").length;
        var halfstarElt = e.querySelector(".half-star-rating");
        var partial = 0;
        if (e.querySelector(".half-star-rating"))
            partial = Number(e.querySelector(".half-star-rating").style.width.replace("%",""))/100;
        return whole + partial;
    };
    removeSelectorFiltered(
        ".product-list>li",
        e => nStars(e) < n
    );
}
function danmurphysEscape() {
    if (getFirstSelector("#wish_list")) clickSelector(".fa-heart");
    clickSelector(".player-close");
}
function danmurphysScrollToReviews() {
    var reviews = getFirstSelector("#customer-review-container");
    reviews && reviews.scrollIntoView();
}

function danmurphysMarkupOnlineOffers(limit=1) {
    var products = getSelectors("ul.product-list shop-product-card");
    for ([i,p] of products.entries()) {
        var prices = p.querySelector("div.price-container");
        if (!prices) continue; "member offer";
        var promo = prices.getElementsByClassName("promo-price");
        if (!promo || promo.length==0) continue;
        else var promoPriceElt = promo[0];
        var promoPrice = parseFloat(promoPriceElt.getElementsByClassName("value")[1].textContent.slice(1));
        var allPriceElts = Array.from(prices.getElementsByClassName("price"));
        var priceElt = allPriceElts.find(e=>!e.classList.contains("promo-price")&&e.innerText.match("per bottle"));
        if (!priceElt) continue; "unit is not a bottle";
        var price = parseFloat(priceElt.getElementsByClassName("value")[0].textContent.slice(1));
        if (price-promoPrice < limit) {
            var ribbon = p.getElementsByClassName("ribbon")[0];
            if (ribbon) ribbon.remove();
            p.querySelector("span.value.pre-text").style.textDecoration="line-through";
        }
    }
}
EOF

command! danmurphysnstars -nargs=1 -js danmurphysFilterProductListByStars(args[0]);

map ]] -description "Next Page"
\    -js clickSelector("div.paging-info>a>i.icon-chevron-right") && danmurphysScrollToReviews();
map [[ -description "Previous Page"
\    -js clickSelector("div.paging-info>a>i.icon-chevron-left") && danmurphysScrollToReviews();
map <site>c -description "(product) expand reviews, (wishlist) toggle pull-down" -js danmurphysGo();
map <site>n -description "Show only products with >=N stars" -b :danmurphysnstars<Space>
map <site>o -description "(show bindings: Goto)" -js CommandExMode().open("showkeys <site>o")
map <site>oc -description "Go to cart" -js openOrSelectTab("https://www.danmurphys.com.au/dm/ecart/cart", count?"tab":"here", null, true);
map <site>or -description "Scroll to reviews" -js danmurphysScrollToReviews();
map <site>ow
\    -description "Go to Wish List"
\    -js openOrSelectTabAndRunCode(
\            "https://www.danmurphys.com.au/dm/mydans/my_lists",
\            (isNew)=>{ if (isNew) window.setTimeout(()=>clickSelectorByText("span", "Wish List"), 2000) }
\        );
map <site>so -description "Style offers with negligible discount" -js danmurphysMarkupOnlineOffers();
map <site>v -description "View/close video" -js clickSelector(".icon-video")||clickSelector(".player-close");
map <site>* -description "Add to list" -js clickSelector(".icon-heart") || clickSelector(".fa-heart");
