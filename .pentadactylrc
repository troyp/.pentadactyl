map <site>os -description "View image: switch to smooth" -js figurosityViewSwitch("smooth");
map <site>ou -description "View image: switch to nude"   -js figurosityViewSwitch("nude");
map <site>v
\    -description "View"
\    -js dactyl.open(figurosityGetImageUrl());
map <site>y      -description "(show bindings: site yank)" -js CommandExMode().open("showkeys <site>y")
map <site>y<C-h> -description "(show bindings: site yank)" -js CommandExMode().open("showkeys <site>y")
map <site>yd
\    -description "Yank image download (to set directory) command"
\    -js utils.cbWrite(`wget -P "${figurosityDownloadDir}" ${figurosityGetImageUrl()}`);
map <site>yi
\    -description "Yank image source"
\    -js utils.cbWrite(figurosityGetImageUrl());
map <site>yn
\    -description "Yank identifying numbers from source URL"
\    -js utils.cbWrite(figurosityGetIdentifyingNumbers());
map <site>yp
\    -description "Yank image download commands for all angles/variants of current pose (sleep between dls, unless count)"
