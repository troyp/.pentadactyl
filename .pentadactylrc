command! kanshudonamesearch -bang -nargs 1 -js kanshudoNameSearch(args[0], bang?"adjacent":"here");
command! kanshudosearchgrammar -bang -nargs + -js kanshudoSearchGrammar(args, bang?"adjacent":"here");
command! kanshudoexamplesearch -bang -nargs + -js kanshudoExampleSearch(args, bang?"adjacent":"here");
