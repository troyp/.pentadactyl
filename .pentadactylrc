" -------------------------------------------------------------------------------
" ,------------,
" | Figurosity |
" '------------'
group figurosity -locations=figurosity.com

js <<EOF
var figurosityDownloadDir = homedir + "/ebooks/art/_images/figurosity";

function figurosityGetImageUrl() {
    var imgHolder = getSelectors("#image-holder>img");
    if (imgHolder.length) return imgHolder[0].src;
    else return buffer.URL;
}
function figurosityDownloadImage(dir) {
    io.system(`wget -P "${dir}" ${figurosityGetImageUrl()}`);
}
function figurosityGetPose() {
    var imgElt = figurosityGetImageUrl();
    var match = imgElt.src.match(/pose-(\d+)/);
    return match.length>0 && parseInt(match[1]);
}
function figurosityGetIdentifyingNumbers() {
    /* Return the identifying numbers preceding "normal" etc in an image src URL for a pose.
       Converts slashes to underscores. */
    var imgElt = figurosityGetImageUrl();
    var match = imgElt.src.match(/.*muses\/(.*)\/(normal|nude|muscle|smooth).*/);
    return match.length>0 && match[1].replace(/\//g, "_");
}
function figurosityDownloadPoseCmd(sleep=true) {
    var s = "";
    var src = figurosityGetImageUrl();
    var regex = /(.*\/)(normal|nude|muscle|smooth)(.*pose-)(\d+)(.*)/;
    var matches = src.match(regex);
    for (i=0; i<=35; ++i) {
        s += "wget " + matches[1] + "normal" + matches[3] + (i<10?"0":"") + String(i) + matches[5] + (sleep ? "; sleep 1;\n" : ";\n");
        s += "wget " + matches[1] + "nude"   + matches[3] + (i<10?"0":"") + String(i) + matches[5] + (sleep ? "; sleep 1;\n" : ";\n");
        s += "wget " + matches[1] + "muscle" + matches[3] + (i<10?"0":"") + String(i) + matches[5] + (sleep ? "; sleep 1;\n" : ";\n");
        s += "wget " + matches[1] + "smooth" + matches[3] + (i<10?"0":"") + String(i) + matches[5] + (sleep ? "; sleep 1;\n" : ";\n");
    }
    return s;
}
function figurosityViewSwitch(type="normal") {
    var url = figurosityGetImageUrl();
    dactyl.open(url.replace(/\/(normal|nude|muscle|smooth)\//, "/"+type+"/"));
}
EOF

map <site>dd
\    -description "Download image"
\    -js figurosityDownloadImage(figurosityDownloadDir);
map <site>o      -description "(show bindings: view image: switch)" -js CommandExMode().open("showkeys <site>o")
map <site>o<C-h> -description "(show bindings: view image: switch)" -js CommandExMode().open("showkeys <site>o")
