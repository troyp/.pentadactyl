js <<EOF
function showClosedWindows() {
    var clwins = JSON.parse(nsISessionStore.getClosedWindowData());
    var clwindescs = clwins.map((w, i)=>
    [
        `${i+1}`,
        `${w.tabs.length} tabs`,
        w.title,
        `${new Date(w.closedAt)}`
    ].join("|"));
    utils.messageWithLineLimit(clwindescs.join("\n"));
}
EOF


