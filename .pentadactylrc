js <<EOF
function redditLoadMoreReplies() {
    getSelectorByText("p","^[0-9]+ more repl(ies|y)").forEach(e=>e.click())
}

function redditTidy()   {
    redditLoadMoreReplies();
    var divZeroTabindex = getSelectorWithMatchingAttribute("div","tabindex",0);
    var articleContent = divZeroTabindex.length && divZeroTabindex[0].parentElement;
    if (articleContent) {
        topElements().forEach(elt => elt.remove());
        content.document.body.appendChild(articleContent);
    } else {
        utils.message("Warning: in RedditTidy(): articleContent not an element");
    }
    var allDivs = getSelectors("div");
    allDivs.forEach(d=>(Array.from(d.children)).every(e=>e.nodeName=="BUTTON") && d.remove());
    removeSelectors(
        "icon", "button",
        "body>div>div>div:nth-of-type(2)",
        "body>div>div>div:nth-of-type(3)"
    );
}


function redditToggleSave() {
    var saveBtn = content.document.querySelector('.link-save-button>a');
    var unsaveBtn = content.document.querySelector('.link-unsave-button>a');
    (saveBtn || unsaveBtn).click();
}
EOF

