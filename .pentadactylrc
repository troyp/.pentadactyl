command! thesauruspower -description "Search PowerThesaurus.com" -nargs + -js thesaurusPower(args.join(" "));
command! thesaurusaik -description "Search Aiksaurus" -nargs + -js thesaurusAik(args.join(" "));
command! thesauruswiktionary -bang -nargs + -js thesaurusWiktionary(args.join(" "));
command! localdict -nargs + -js utils.messageWithLineLimit(localdict(args.join(" ")) || "no results found");

js <<EOF
function localdict(s) {
    var gcide = io.system(`dict -d gcide '${s}' | perl -000 -lpe 's/ *\n */ /g'`);
    var wn = io.system(`dict -d wn '${s}'| perl -000 -lpe 's/ *\n */ /g'`);
    var output = "";
    if (gcide.returnValue==0) output += gcide.output;
    if (wn.returnValue==0) output += wn.output;
    return output;
}
function thesaurusAik(s, wrap=160) {
    cmd = `aiksaurus ${s}`;
    var res = io.system(cmd);
    var out = "";
    var lines = S.toLines(res.output, wrap);
    utils.message(lines.join("\n") + "\n");
}
function thesaurusPower(s, wrap=160) {
    cmd = `curl -sA "Mozilla/5.0 (X11; Linux i686; rv:64.0) Gecko/20100101 Firefox/64.0" "https://www.powerthesaurus.org/${s}/synonyms" | pup ".pt-thesaurus-card__term-title text{}" | paste -sd ","`;
    var res = io.system(cmd);
    utils.message(S.toLines(res.output, wrap).join("\n"));
}
function thesaurusWiktionary(s, wrap=190) {
    "TODO: Fix function for multiword arguments"
    var url = `https://en.wiktionary.org/wiki/Special:Search?search=${s}&ns110=1&fulltext=Search+Thesaurus`
    cmd = `curl -s '${encodeURIComponent(url)}' |` +
          ` pup '.mw-search-result json{}' |` +
          ` fx '.map(x=>x.children[0].children[0].title+": "+x.children[1].text).join("\\n")' |` +
          ` perl -n -mHTML::Entities -e ' ; print HTML::Entities::decode_entities(\$_) ;'`;
    var res = io.system(cmd);
    utils.message(S.toLines(res.output, wrap).join("\n"));
}
EOF


