\    -description "Open last download and set default application for type"
\    -count
\    -js mimeopenAndSetAppForLastDownload(count||1)
map <app>dv
\    -description "Open last download in browser"
\    -count
\    -js openLastDownloadInBrowser(count||1)
map <app>dy
\    -description "Yank last download path"
\    -count
\    -js getLastDownload(count||1).then(function (dlPath) { dactyl.utils.cbWrite(dlPath); });

js <<EOF
function getLastDownload(n=1) {
    dlListP = Downloads.getSummary(Downloads.ALL);
    var resultP = dlListP.then(function (dlList) {
        return dlList._downloads[dlList._downloads.length - n].target.path;
    });
    return resultP;
}
window.getLastDownload = getLastDownload;

function mimeopenLastDownload(n=1) {
    var dlP = getLastDownload(n);
    dlP.then(function(dl) { io.system(`mimeopen -n '${dl}'`); });
}
window.mimeopenLastDownload = mimeopenLastDownload;

function mimeopenAskAppForLastDownload(n=1) {
    var dlP = getLastDownload(n);
    dlP.then(function(dl) { io.system(`roxterm -e zsh -c "mimeopen -a '${dl}'"`); });
}

function mimeopenAndSetAppForLastDownload(n=1) {
    var dlP = getLastDownload(n);
    dlP.then(function(dl) { io.system(`roxterm -e zsh -c "mimeopen -d '${dl}'"`); });
}

function trashLastDownload(n=1) {
    var dlP = getLastDownload(n);
    dlP.then(function(dl) {
        var cmd=io.system(`trash '${dl}'`);
        if (cmd.returnValue==0) utils.message("Trashed " + dl);
        else utils.message("Failed to trash " + dl);
    });
}

function openLastDownloadAsMaff(n=1) {
    var dlP = getLastDownload(n);
    dlP.then(f=> openMaffFile(f));
}

function openLastDownloadDir(n=1) {
    var dlP = getLastDownload(n);
    dlP.then(f=> io.system(`dolphin $(dirname "${f}")`));
}

function openLastDownloadDirInBrowser(n=1) {
    var dlP = getLastDownload(n);
    dlP.then(f=> dactyl.open(`file://${f.replace(/[^/]*$/, "")}`, "tab"));
}

function openLastDownloadInBrowser(n=1) {
    var dlP = getLastDownload(n);
    dlP.then(f=> openOrSelectTab(`file://${f}`));
}

function renameLastDownload(n=1) {
    var dlP = getLastDownload(n);
    dlP.then(f=> {
        utils.message(
            io.system(
                `mv ${f} $(zenity --entry --text "Rename" --entry-text="${f}" 2>/dev/null)`
            ).output);
    })
}
EOF

