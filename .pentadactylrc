js <<EOF
function imageTransform(type) {
    var name = buffer.URL.split("/").pop();
    var tempdir = io.system("mktemp -d").output;
    var path = tempdir+"/"+name;
    dactyl.execute(`saveas ${path.replace(" ", "\ ")}`);
    " type = argument to mogrify, eg. 'flip' (V reflect), 'flop' (H reflect), 'rotate DEG' ";
    io.system(`mogrify -${type} '${path}'`);
    dactyl.open("file://"+path);
}
EOF

