function newtownartCount() {
    var qtyBoxes = Array.from(content.document.querySelectorAll(".CartItemQuantity input"));
    if (qtyBoxes.length) {
        var quantities = qtyBoxes.map(e=>Number(e.value));
        return quantities.reduce((n,s)=>n+s);
    }
}
