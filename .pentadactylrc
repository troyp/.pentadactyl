    \ -description "PDF: Yank specified text divs (eg. 2-5,11,19)"
    \ -nargs=1
    \ -js pdfYankText(parseRanges(args[0]));
command! pdfYankLines
    \ -description "PDF: Yank specified lines (eg. 2-5,11,19)"
    \ -nargs=1
    \ -js pdfYankLines(parseRanges(args[0]));

js <<EOF
function yankTextarea(n) {
    var contents = getTextArea(n);
    var contents = getSelectorContent(`textarea:nth-of-type(${n})`);
    utils.cbWriteWithMessageLimit(contents);
}

function yankSelectorContent(sel, target='innerText') {
    utils.cbWriteWithMessageLimit(getSelectorContent(sel, target).join("\n"));
}

function yankSelectorCompletions(...sels) {
    return getSelectorsContent(sels).map(e=>[e]);
}

function yankSelectorAttributeCompletions(attr, ...sels) {
    return getSelectorsAttributeContent(attr, sels).map(e=>[e]);
}

function pdfGetText(divNums, divSep=" ", lineSep="\n") {
    var textDivs = getSelectors('.textLayer>div');
    included = divNums.map(n=>textDivs[n-1]);
    var top = null;
    var result = "";
    included.forEach(e=>{
        if (top) result += (e.style.top > top) ? lineSep : divSep;
        result += e.textContent;
        top = e.style.top;
    });
    return result;
}

function pdfYankText(lines, divSep=" ", lineSep="\n") { utils.cbWriteWithMessageLimit(pdfGetText(lines, divSep, lineSep)); }

function pdfGetLines(lines, divSep=" ", requireLowerPos=true) {
    var last = lines.slice(-1);
    var textDivs = getSelectors('.textLayer>div');
    var result = [];
    var top = null;
    var line = "";
    var count = 1;
    for (div of textDivs) {
        var testPos = requireLowerPos ? (div.style.top > top) : (div.style.top != top);
        if (testPos) {
            count++;
            if (count > last) { return result; }
            if (lines.includes(count)) { result.push(line); }
            line = "";
        }
        if (line) line += divSep;
        line += div.textContent;
        top = div.style.top;
    }
    return result;
}
window.pdfGetLines = pdfGetLines;

function pdfYankLines(lines, lineSep="\n", divSep=" ", requireLowerPos=true) {
    var s = pdfGetLines(lines, divSep, requireLowerPos).join(lineSep);
    utils.cbWriteWithMessageLimit(s);
}
window.pdfYankLines = pdfYankLines;

function yankCollection(a, transform=null) {
    var s;
    if (transform)
        s = Array.from(a).map(transform).join("\n");
    else
        s = Array.from(a).join("\n");
    utils.cbWriteWithMessageLimit(s);
    return s;
}

function getInputFields() {
    var inputElts = Array.from(content.document.getElementsByTagName("input"));
    var textFields = inputElts.filter(
        e => [
            "text", "search", "email", "url", "file",
            "time", "date", "color", "number"
        ].includes(e.type));
    var textInput = textFields.filter(e => e.value!="");
    return textInput;
}
function yankInputFieldsRec(textInput) {
    if (textInput.length == 0) return;
    dactyl.clipboardWrite(textInput[0].value);
    window.setTimeout(
        () => yankInputFieldsRec(textInput.slice(1)),
        250
    );
}
function yankInputFields() {
    var textInput = getInputFields();
    yankInputFieldsRec(textInput);
    utils.message("Copied\n" + textInput.map(f=>"    "+f.value).join("\n"));
}

function yankInputFieldN(n) {
    var textInput = getInputFields();
    var N = textInput.length;
    if ( N >= n)
        utils.yankWithMsg(textInput[n-1].value);
    else
        utils.message(`Input field ${n} does not exist. There are only ${N} input fields.`);
}

function yankTabsList({index=true, label=true, url=true}={}) {
    var alltabs = tabs.allTabs;
    var alltabsitems =
        alltabs.map((t, i)=>
            `${index?i:""}: ${label?t.label:""}   <${url?t.linkedBrowser.contentDocument.URL:""}>`
        );
    var resultstr = alltabsitems.join("\n");
    utils.cbWriteWithMessageLimit(resultstr);
}

EOF

