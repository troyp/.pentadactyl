js <<EOF
function courseraShowMore() {
        clickSelectorByTextAll('button', /show more/i);
        window.setTimeout(clickSelectorByTextAll('button', /show all/i), 300);
    }
function courseraToggleSeeMore() {
    if (clickSelectorByTextAll('button', /see all/i)) return true;
    clickSelectorByTextAll('button', /see less/);
    return false;
}

function courseraGo() {
    var url = buffer.URL;
    if (url.match(/https:\/\/www\.coursera\.org\/learn\//)) {
        courseraShowMore();
        courseraToggleSeeMore();
    }
}
EOF

