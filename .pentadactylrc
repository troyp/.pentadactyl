\    -description "Save clipboard text to file. :savecbtext NAME DIR"
\    -nargs +
\    -complete dir
\    -js saveClipboardTextToFile(args[0], args[1]);

js <<EOF
function saveClipboardTextToFile(name, dir=homedir) {
    var text = readFromClipboard();
    var path = (dir+"/"+name).replace(/rr/,"r");
    var testS = `[[ -e "${path}" ]]`;
    var errS = 'echo "File already exists"';
    var saveS = `echo "${text}" > "${path}"`;
    var succmessS = `echo "Saved text to ${path}.\n"; head -n 10 "${path}"`;
    utils.message(io.system(`if ${testS}; then ${errS}; else ${saveS}; ${succmessS}; fi`).output);
}
EOF

