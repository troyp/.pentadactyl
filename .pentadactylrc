js <<EOF
function isolateArticle(conservative=false) {
    var keep = hasSelectors("article:first-of-type", ".article", "#main", "#content");
    if (keep.length) isolateSelectors(keep[0]);
    removeSelectors(".sharedaddy");
    !conservative && removeSelectors("footer");
}
EOF

