" ,------------------,
" | Buffer Selection |
" '------------------'

command! openorselect -description "Open URL in current (! new) tab, or switch to existing tab"
\  -nargs=+
\  -bang
\  -complete=url
\  -js openOrSelectTab(args[0], args[1]?"here":"tab");
command! openorsummon -description "Open URL in new tab, or move existing tab after current. Select unless !"
\  -nargs=+
\  -bang
\  -complete=url
\  -js openOrSummonTab(args[0], bang, args[1]||null);

js <<EOF
function tabSelectLeft(n=1, wrap=true) {
    var currIdx = tabs.getTab()._tPos;
    targetIdx = currIdx - n;
    tabs.select(targetIdx);
    return targetIdx;
}
function tabNextMatchingDomain(n=1, ignoreSubdomains=true) {
    var current = tabs.getTab();
    for (var i=0; i<n; ++i) {
        var matching = tabs.allTabs.filter(t=>urlDomain(t.linkedBrowser.contentDocument.URL)==urlDomain(buffer.URL));
        current = matching.find(t=>t._tPos>current._tPos) || matching[0];
    }
    tabs.select(current);
}
function tabPreviousMatchingDomain(n=1, ignoreSubdomains=true) {
    var current = tabs.getTab();
    for (var i=0; i<n; ++i) {
        var matching = tabs.allTabs.filter(t=>urlDomain(t.linkedBrowser.contentDocument.URL)==urlDomain(buffer.URL)).reverse();
        current = matching.find(t=>t._tPos<current._tPos) || matching[0];
    }
    tabs.select(current);
}
EOF

