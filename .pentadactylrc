" ====== "
" Tables "
" ====== "
set eht+=[τ]:table
js <<EOF
hints.addMode(
    'τ','show number of table rows',
    t => {
        var tbodies = Array.from(t.tBodies);
        var rows = [].concat(...tbodies.map(b => Array.from(b.children)));
        var dataRows = rows.filter(r => Array.from(r.children).some(c=>c.nodeName=="TD"));
        utils.message(`TABLE: ${dataRows.length} rows`);
    }
);
EOF

