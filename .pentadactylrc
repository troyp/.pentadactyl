js <<EOF
function jacksonsartCount() {
    var elts = getSelectorWithMatchingAttribute('input', 'title', 'Qty');
    if (elts.length) {
        return elts.map(e=>Number(e.value)).reduce((n,s)=>n+s);
    }
}
function jacksonsartItemNames() {
    return getSelectors(".cart .product-name").map(e=>e.innerText.replace("\n", "\t"));
}
function jacksonsartItemUrls() {
    return getSelectors(".cart .product-name>a").map(e=>e.href);
}
function jacksonsartGetCartTotal() {
    var s = getSelectorByText('td',/grand total/i)[0].nextElementSibling.textContent.trim();
    return Number(s.match(/\$(\d+\.\d\d)/)[1]);
}
function jacksonsartToggleAU() {
    var url = buffer.URL;
    toggleUrl("www.jacksonsart.com/en-au/", "www.jacksonsart.com/");
}
function jacksonsartShowShippingInfo(where) {
    openAndRunCode("https://www.jacksonsart.com/en-au/shipping-info", ()=>getSelectorByText("h2",/international shipping/i)[0].scrollIntoView());
}
function jacksonsartToggleHairType(hair) {
    /* Change hair type query component to HAIR if set to a different hair, or add it if missing.
       If already set to HAIR, remove the query component. */
    var url = buffer.URL;
    var newurl;
    var hairtypeRegex = /fq\[x_hairtype\]=([^?&]+)/;
    var match = url.match(hairtypeRegex);
    var newQuery = "fq[x_hairtype]="+hair;
    if (match) newurl = url.replace(hairtypeRegex, newQuery);
    else if (url.match(/\?/)) newurl = url + "&" + newQuery;
    else newurl = url + "?" + newQuery;
    dactyl.open(newurl);
}

function jacksonsartGo(count=undefined) {
    var url = buffer.URL;
    if (url=="https://www.jacksonsart.com/en-au/sales/order/history/") {
        var href = getFirstSelectorByText("#my-orders-table>tbody>tr.first a", "View Order").href;
        dactyl.open(href+"#my-orders-table", count?"tab":"here");
    } else if (url=="https://www.jacksonsart.com/en-au/checkout/cart/") {
        clickSelectorByText("button", "Proceed to Checkout");
    }
}
EOF

