function copyTextContent(e) {
   var t = e.textContent.trim().replace(/(\s*\n\s*)(\s*\n\s*)+/g, "\n\n");
    utils.cbWriteWithMessageLimit(t);
}

hints.addMode(
    "Π",
    "Yank post",
    e => copyTextContent(e)
);
EOF

