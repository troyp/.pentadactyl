function qpTogglePause() {
    var pauseBtn = content.document.querySelector(".btn-pause");
    var continueBtn = content.document.querySelector(".btn-continue");
    if (pauseBtn.style.display=="none") continueBtn.click();
    else pauseBtn.click();
}
