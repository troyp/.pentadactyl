js <<EOF

function jCopyCellToPrompt(cell) {
    var cellText = cell.innerText;
    var prompt = content.document.getElementById("prompt");
    prompt.innerText = cellText;
    buffer.followLink(prompt, dactyl.CURRENT_TAB);
}

EOF

" === hint modes === "
