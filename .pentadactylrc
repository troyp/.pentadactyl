js <<EOF
function wolframGetResult() {
    var ptdiv = content.document.getElementById('plaintext');
    if (ptdiv)
        utils.yankWithMsg(ptdiv.innerText);
    else
        content.document.querySelector('#Result .plaintext').click();
}
EOF

