map <leader>+ -b :sum<Space>
map <leader>* -b :product<Space>

js <<EOF
function convertUnits(n, unit1, unit2) {
    return Number(io.system(`units -t "${n} ${unit1}" ${unit2}`).output);
}
function convertExchangeRates(n, FROMCURR, TOCURR) {
    var f = FROMCURR.toUpperCase();
    var t = TOCURR.toUpperCase();
    var querystr = `q=${f}_${t}&compact=ultra&apiKey=${user.mycurrencyconverterapikey}`;
    var cmd = `curl -s "https://free.currencyconverterapi.com/api/v5/convert?${querystr}"`;
    var result = io.system(cmd).output;
    var regex = RegExp(`{"${f}_${t}":([0-9.]+)}`);
    var match = result.match(regex);
    /*  alert(`${f}\n${t}\n${querystr}\n${cmd}\n${result}\n${regex}\n${match}`);  */
    return match && Number(match[1]) * n;
}
function convertUnitsWrapper(...args) {
    var n, unit1, unit2;
    if (args.length==2) {
        var match = args[0].match(/(\d*)(.*)/);
        n = match[1]||"1";
        unit1 = match[2];
        unit2 = args[1];
    } else {
        [n, unit1, unit2] = args;
    }
    var res = convertUnits(n, unit1, unit2);
    return n + unit1 + " = " + res + unit2;
}

var paperSizes = [
    ["4A0", "1682×2378 mm	66.2×93.6 in"],
    ["2A0", "1189×1682 mm	46.8×66.2 in"],
    ["A0", "841×1189 mm	33.1×46.8 in"],
    ["A1", "594×841 mm	23.4×33.1 in"],
    ["A2", "420×594 mm	16.5×23.4 in"],
    ["A3", "297×420 mm	11.7×16.5 in"],
    ["A4", "210×297 mm	8.3×11.7 in"],
    ["A5", "148×210 mm	5.8×8.3 in"],
    ["A6", "105×148 mm	4.1×5.8 in"],
    ["A7", "74×105 mm	2.9×4.1 in"],
    ["A8", "52×74 mm	2.0×2.9 in"],
    ["A9", "37×52 mm	1.5×2.0 in"],
    ["A10", "26×37 mm	1.0×1.5 in"]
];
EOF

" Download PDF "
command! dlpdf
\    -nargs=1
\    -complete=dir
\    -js dlPDF(args[0]);
js <<EOF
function dlPDF(dir) {
    var title = content.document.querySelector('.textLayer>div').textContent;
    var safeTitle = title.replace("\n", " - ").replace(/\//g, "∕");
    var path = dir.replace(/\/?$/, "/") + safeTitle;
    CommandExMode().open(`saveas ${path}.pdf`);
}
EOF

js <<EOF
function wikipediaGotoPage(s, where="tab", openorselect=true) {
    var page = strToWikiTitle(s);
    var url = `https://en.wikipedia.org/wiki/${page}`;
    if (openorselect)
        openOrSelectTab(url, where);
    else
        dactyl.open(url, { where: where });
}
function wiktionaryGotoPage(s, where="tab", openorselect=true) {
    var page = strToWikiTitle(s, false);
    var url = `https://en.wiktionary.org/wiki/${page}`;
    if (openorselect)
        openOrSelectTab(url, where);
    else
        dactyl.open(url, { where: where });
}
EOF


