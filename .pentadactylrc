" -----------------------------------------------------------------------------
" ,---------------,
" | Wikipedia.org |
" '---------------'
group wikipedia -locations=wikipedia.org,wikisource.org,wiktionary.org,wikibooks.org,wikimedia.org

js <<EOF
var wpLatest = function() {
   var newurl = buffer.URL.replace(/\?.*$/, '');
   dactyl.open(newurl);
}
var wpContent = function() {
    var heading = content.document.getElementById("firstHeading");
    var articletext = content.document.getElementById("bodyContent");
    return heading.innerText + "\n\n" + articletext.innerText;
}
var wpHtml = function() {
    var heading = content.document.getElementById("firstHeading");
    var articletext = content.document.getElementById("bodyContent");
    return `<h1>${heading.innerHTML}</h1>\n${articletext.innerHTML}`;
}
var wpYankContent = function() {
    articletext = wpContent();
    dactyl.clipboardWrite(articletext);
    dactyl.utils.message(`COPIED: ${articletext.slice(0, 20)} ... ${articletext.slice(-20)}`);
}
var wpYankHtml = function() {
    articlehtml = wpHtml();
    dactyl.clipboardWrite(articlehtml);
    dactyl.utils.message(`COPIED: ${articlehtml.slice(0, 20)} ... ${articlehtml.slice(-20)}`);
}

function wpGetContents() {
    return Array.from(content.document.querySelectorAll('#toc ul li a'));
}

function wpCleanup() {
    removeSelectors("#siteNotice", "#mw-navigation", "#footer", ".noprint", ".mw-editsection", ".navbox", "#xtools", "#kanjiInfo");
    getFirstSelectorByText("a", "Search Google").remove();
}

function wpGotoNextHeading() {
    var headings = wpGetContents();
    var fragment = content.window.location.hash;
    if (fragment) {
        var i = headings.findIndex((e,i,a)=>e.href.match(content.window.location.hash));
        if (i+1 < headings.length)
            headings[i+1].click();
    } else {
        headings[0].click();
    }
}

function wpGotoPreviousHeading() {
    var headings = wpGetContents();
    var fragment = content.window.location.hash;
    if (fragment) {
        var i = headings.findIndex((e,i,a)=>e.href.match(content.window.location.hash));
        if (i)
            headings[i-1].click();
        else
            content.window.scrollTo(0, 0);
    }
}

function wpGetSections() {
    var contents = wpGetContents();
    var n = contents.length;
    var max_n1 = Math.max.apply(null, contents.map(e=>e.innerText.match(/^[0-9]+/)[0].length));
    var formatted = contents.map(e => {
        var padding = max_n1 - e.innerText.match(/^[0-9]+/)[0].length;
        var fmtElt = S.repeat("0", padding) +
                      decodeURIComponent(
                          S.padEnd(e.innerText.match(/[0-9.]+/)[0], 10, "…") +
                          e.href.replace(/[^#]+#/, ""));
        return [ fmtElt, "" ];
    });
    return formatted;
}

function wpGetTopicOrSearchTerm() {
    var url = buffer.URL;
    var regex = /https:\/\/([^./]+)\.wik.*\.org\/wiki\/([^?#]+)/i;
    var regex2 = /https:\/\/([^./]+)\.wik.*\.org\/w\/index.php\?(search|title)=([^&]+)/i;
    var match = url.match(regex);
    var match2 = url.match(regex2);
    if (match) return match[2];
    else if (match2) return match2[3];
    else return null;
}

function wpGotoWiktionary(where='here', lang='en') {
    var topic = wpGetTopicOrSearchTerm().toLowerCase();
    dactyl.open(`https://${lang}.wiktionary.org/w/index.php?search=${topic}`, where);
}

function wpGotoWikipedia(where='here', lang='en') {
    var topic = wpGetTopicOrSearchTerm().toLowerCase();
    dactyl.open(`https://${lang}.wikipedia.org/w/index.php?search=${topic}`, where);
}
}

function wpGetSearchCommandForSite(url=buffer.URL, invert=false) {
    if (invert)
        return url.match("wiktionary.org/") ? "wpsearch" : "wiktionarysearch";
    else
        return url.match("wiktionary.org/") ? "wiktionarysearch" : "wpsearch";
}

function wpParseURL(url=buffer.URL) {
    var parts = url.match(/https:\/\/([^./]+)\.(wik.*)\.org\/(.*)/i);
    var [,subdomain, siteName, path] = parts;
    /* find title */
    var regex1 = /^wiki\/([^?#]+)/i;
    var regex2 = /^w\/index.php\?title=([^&]+)/i;
    var match = path.match(regex1) || path.match(regex2);
    var title = match && match[1];
    return {
        url:url, subdomain:subdomain, siteName:siteName, path:path, title:title,
        toString: function() { return this.url; }
    };
}
window.wpParseURL = wpParseURL;

function wpGetUserPageHREF() { return getFirstSelector("#pt-userpage>a").href; }

function wpGetUser() {
    var upageHref = wpGetUserPageHREF();
    return upageHref.match(/\/User:(.*)/)[1];
}

EOF

